{
    "manifest_version": 3,
    "name": "Pomodoro Timer",
    "version": "1.0.0",
    "description": "A Timer Extension",

    "icons":{
        "16":"unnamed.png",
        "48":"unnamed.png",
        "128":"unnamed.png"
    },
    "action":{
        "default_icon":{
            "16":"unnamed.png",
            "24":"unnamed.png",
            "32":"unnamed.png"
        },
        "default_title":"Pomodoro Extension Action",
        "default_popup":"popup.html"
    },
    "options_page": "options.html",
    "permissions": ["storage","alarms", "notifications"],
    "background":{
        "service_worker":"background.js"
    }
}